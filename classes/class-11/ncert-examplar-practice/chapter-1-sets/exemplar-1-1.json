                {
                    "id": "exemplar-11-ch1-41",
                    "number": 13,
                    "question": "If A and B are two sets, then \\(A \\cap (A \\cup B)'\\) equals to",
                    "options": [
                        "\\(A\\)",
                        "\\(B\\)",
                        "\\(\\phi\\)",
                        "\\(A \\cap B\\)"
